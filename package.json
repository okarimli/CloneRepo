    "scripts": {
        "lint": "eslint '**/*.{ts,tsx}'",
        "lint:fix": "eslint '**/*.{ts,tsx}' --fix",
        "lint:style": "stylelint '**/*.scss'",
        "lint:style:fix": "stylelint '**/*.scss' --fix",
        "format": "prettier --write '**/*.{ts,tsx,scss,css,js,jsx,json}'",
        "format-prettier": "prettier --eslint-config-path .eslintrc.js --write '**/*.{ts,tsx,scss,css,js,jsx,json}'",
        "build:dev": "webpack --config configs/webpack.prod.js --env buildEnv=dev",
        "build": "webpack --env buildEnv=prod --config configs/webpack.prod.js",
        "build:for-preview": "webpack --env buildEnv=dev apiUrl=https://dmzdev.pashabank.az --config configs/webpack.prod.js",
        "build:with-source-maps": "webpack --env buildEnv=prod --config configs/webpack.prod.js",
        "build:analyze": "webpack --env.analyze --config configs/webpack.prod.js",
        "start-server": "cd ./server && npm run-script serve:watch",
        "start-server:acquiring": "cd ./src/modules/Acquiring/server && npm run-script serve:watch",
        "start-server:vpos": "cd ./src/modules/VirtualPos/server && npm run-script serve:watch",
        "start-server:salary": "cd ./src/modules/Salaries/server && npm run-script serve:watch",
        "start-server:oba": "cd ./src/modules/OBA/server && npm run-script serve:watch",
        "start-server:company-profile": "cd ./src/modules/CompanyProfile/server && npm run-script serve:watch",
        "start-server:umico-pos": "cd ./src/modules/UmicoPOS/server && npm run-script serve:watch",
        "start": "concurrently --kill-others \" webpack serve --env isLocalEnv=true --config configs/webpack.dev.js \" \" npm run start-server \" \" npm run start-server:acquiring \" \" npm run start-server:salary \" \" npm run start-server:oba \" \" npm run start-server:company-profile \" \"npm run start-server:vpos \" \" npm run start-server:umico-pos \"",
        "start:with-prod-api": "webpack serve --env apiUrl=https://pashabank.digital --config configs/webpack.dev.js",
        "test": "jest --config jest.config.json",
        "test:cov-report-sonar": "npm run test -- --coverage --testResultsProcessor=jest-sonar-reporter",
        "test:changedSinceDevelop": "npm run test -- --changedSince=origin/develop",
        "test:watch": "jest --watchAll --config jest.config.json",
        "tsc": "tsc --noEmit -p ./tsconfig.json",
        "tsc:watch": "tsc --noEmit --watch -p ./tsconfig.json",
        "tools:build": "tsc -p tools --outDir tools/build",
        "tools:platform-version:watch": "ts-node tools/platform-version/index.ts --project ./tsconfig.json",
        "link-library": "npm link @pashabank/ui-library",
        "nls:check": "nls-tools check",
        "configure:csp-headers": "node csp-headers.js",
        "preview": "./preview.sh"
    },
